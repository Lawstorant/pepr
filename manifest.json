    },
		{
            "exclude_globs":    [  ],
            "include_globs":    [ "*" ],
            "js":               [ "pead.js" ],
            "matches": [ "https://pe.szczecin.pl/chapter_201209.asp" ],
            "run_at": "document_end"
    },
		{
            "exclude_globs":    [  ],
            "include_globs":    [ "*" ],
            "js":               [ "pepe.js" ],
            "matches": [ "https://pe.szczecin.pl/chapter_201208.asp" ],
            "run_at": "document_start"
    }],

    "permissions": [
        "https://*/*",
